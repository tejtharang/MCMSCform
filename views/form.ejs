<body>
	<form method="POST" action="#">
	<div class="container">
		<div class="panel panel-info">
			<div class="panel-heading">Introductory Section</div>
			<div class="panel-body">
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-4"> 
							<label class="control-label" for="degreeSelect">
								Doctoral Degree Program
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-4"> 
							<select id="degree" class="form-control" name="degreeSelect" required>
								<option value="initial" disabled selected="selected">Select a degree Program</option>
								<%degrees = ["Applied Mathematics","Mathematics","Mathematics Education","Statistics"];%>
								<%degrees.forEach(function(degree){%>
								<option value=<%=degree%>><%=degree%></option>
								<%});%>    						
							</select>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-4">
							<label class="control-label" for="mastersInPassing">
								Have you applied for Master's in Passing?
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-1">
							<label class="control-label" for="mastersInPassingYes">
								Yes
							</label>
						</div>
						<div class="col-md-1">
							<input type="radio" id="mastersInPassingYes" name="mastersInPassing" value="1" required>
						</div>
						<div class="col-md-1">
							<label class="control-label" for="mastersInPassingNo">
								No
							</label>

						</div>
						<div class="col-md-1">
							<input type="radio" id="mastersInPassingNo" name="mastersInPassing" value="0">
						</div>
					</div>
				</div>
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-4">
							<label class="control-label" for="graduation">
								Are you graduating this year?(Students graduating in summer should answer "yes" as well)
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-1">
							<label class="control-label" for="graduationYes">
								Yes
							</label>
						</div>
						<div class="col-md-1">
							<input type="radio" id="graduationYes" name="graduation" value="1" onclick="displayEmployment()" required>
						</div>
						<div class="col-md-1">
							<label class="control-label" id="graduationNo" for="graduation">
								No
							</label>

						</div>
						<div class="col-md-1">
							<input type="radio" id="graduationNo" name="graduation" value="0" onclick="displaySectionABC()">
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- Employment survey div starts -->
		<div class="panel panel-info" id="employmentDiv" style="display:none;">
			<div class="panel-heading">Employment Survey</div>
			<div class="panel-body">
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-6">
							<label class="control-label" for="emailExtra">
								Please enter an email where you can be contacted after you leave ASU
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-3">
							<input class="form-control" id="emailExtra" type="email" name="emailExtra">
						</div>
					</div>

				</div>
				<div class="row">
						<div class="form-group pad">
							<div class="col-md-5">
								<label class="control-label" for="acceptance">
									Do you have employment after graduation?
								</label>	
							</div>
							<div class="col-md-1">
								<label class="control-label" for="employment">
									Yes
								</label>
							</div>
							<div class="col-md-1">
								<input type="radio" id="employment" name="employment">
							</div>
							<div class="col-md-1">
								<label class="control-label" for="employment">
									No
								</label>

							</div>
							<div class="col-md-1">
								<input type="radio" id="employmentNo" name="employment">
							</div>
						</div>
					</div>

			</div>	
		</div>
		<!-- Section A starts -->
		<div class="panel panel-info" id="sectionADiv" style="display:none;">
			<div class="panel-heading">Section A | Program of Study - Status</div>
			<div class="panel-body">
				<div class="row">
					<div class="col-md-3"> 
						<label class="control-label">
							Academic Year
							<sup class="star">*</sup>
						</label>
					</div>
					<div class="col-md-1"> 
						<label class="control-label" for="academicYearFrom">Fall</label>
					</div>
					<div class="col-md-1" > 
						<input class="form-control" type="text" id="academicYearFrom" name="academicYearFrom" required disabled>
					</div>
					<div class="col-md-2"> 
						<label class="control-label" for="academicYearTo">Spring/Summer</label>
					</div>
					<div class="col-md-1"> 
						<input class="form-control" type="text" id="academicYearTo" name="academicYearTo" required disabled>
					</div>

				</div>
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-3">
							<label class="control-label" for="firstName">
								First Name
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<input class="form-control" type="text" id="firstName" name="firstName" required>
						</div>
						<div class="col-md-2">
							<label class="control-label" for="lastName">
								LastName
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<input class="form-control" type="text" id="lastName" name="lastName" required>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-3">
							<label class="control-label" for="idNumber">
								ASU ID/Student ID Number
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<input class="form-control" type="text" id="idNumber" name="idNumber" required>
						</div>
						<div class="col-md-2">
							<label class="control-label" for="asurite">
								ASURITE ID
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<input class="form-control" type="text" id="asurite" name="asurite" required>
						</div>
					</div>
				</div>

				<div class="row">
					<div class="form-group pad">
						<div class="col-md-3">
							<label class="control-label" for="programStartDate">
								Program Start Date
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<input class="form-control" type="month" id="programStartDate" name="programStartDate" required>
						</div>
						<div class="col-md-2">
							<label class="control-label" for="asurite">
								Semester in Progress
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<select id="currentSemester" class="form-control" name="currentSemester" required>
								<option value="initial" disabled selected="selected">current semester</option>
								<%for(var i=1;i<=20;i++){%>
								<option value=<%=i%>><%=i%></option>
								<%}%>    						
							</select>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-3">
							<label class="control-label" for="gpa">
								Cumulative GPA
								<sup class="star">*</sup>
							</label>	
						</div>
						<div class="col-md-2">
							<input type="text" id="gpa" name="gpa" class="form-control" required>
						</div>
					</div>
				</div>

				<hr>
				<!--<font size="2">
					<i><u>Courses taken previously</u></i>
				</font>
				<table class="table table-hover">
					<tr>
						<th>S.No.</th>
						<th>Course</th>
						<th>Grade</th>
					</tr>
				</table> -->
				<div class="row">
					<div class="form-group pad">
						<div class="col-md-4">
							<label class="control-label" for="coursesAdd">
								Do you want to add courses for the current year?
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-1">
							<label class="control-label" for="coursesAdd">
								Yes
							</label>
						</div>
						<div class="col-md-1">
							<input type="radio" id="coursesAddYes" name="coursesAdd" onclick="displayAddNewCourseDiv()"required>
						</div>
						<div class="col-md-1">
							<label class="control-label" for="coursesAdd">
								No
							</label>

						</div>
						<div class="col-md-1">
							<input type="radio" id="coursesAddNo" onclick="displayNoCourseReasonDiv()" name="coursesAdd">
						</div>
					</div>
				</div>
				<table class="table table-hover pad" id="courseAddTable">
					<tr>
						<th>Course</th>
						<th>Grade</th>
						<th>Action</th>
					</tr>

				</table>
				<div class="row" id="addCourseRowDiv" style="display:none;">
					<div class="form-group pad">
						<div class="col-md-2">
							<label class="control-label" for="coursesName">
								Course Name
								<sup class="star">*</sup>
							</label>
						</div>
						<div class="col-md-2">
							<input type="text" id="courseName" class="form-control" name="courseName">
						</div>
						<div class="col-md-2">
							<label class="control-label" for="courseGrade">
