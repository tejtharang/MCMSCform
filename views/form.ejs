<script>

	$(document).ready(function(){
		var curtime = new Date();
		var month = curtime.getMonth();
		var year = curtime.getFullYear();
		if(month>=8){
			$('#academicYearFrom').val(year);
			$('#academicYearTo').val(year+1);
		}
		else{
			$('#academicYearFrom').val(year-1);
			$('#academicYearTo').val(year);	
		}
	})
</script>
